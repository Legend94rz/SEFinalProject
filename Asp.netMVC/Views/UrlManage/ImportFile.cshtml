
@{
    ViewBag.Title = "文件导入";
}
<div>
    <h2>导入网址配置</h2>

</div>
<div class="col-md-8 col-md-offset-4">

    <div class="add-file-button file form-group">
        <input class="" type="text" size="20" name="upfile" id="upfile" style="border:1px dotted #ccc">
        <input class="btn btn-primary" type="button" value="浏览" onclick="path.click()" style="border:1px solid #ccc;">
        <button class="btn btn-success" style="border:1px solid #ccc;">上传</button>
        <input type="file" accept=".xls,.txt" id="path" style="display:none" onchange="upfile.value=this.value">
        <p class="help-block">只支持Txt/Excel文件。</p>
    </div>
    <div class="col-md-5">
    </div>
</div>
